

public class Mundo {

	/**
	 * @uml.property  name="jugador"
	 * @uml.associationEnd  inverse="mundo:Jugador"
	 */
	private Jugador jugador;

	/**
	 * Getter of the property <tt>jugador</tt>
	 * @return  Returns the jugador.
	 * @uml.property  name="jugador"
	 */
	public Jugador getJugador() {
		return jugador;
	}

	/**
	 * Setter of the property <tt>jugador</tt>
	 * @param jugador  The jugador to set.
	 * @uml.property  name="jugador"
	 */
	public void setJugador(Jugador jugador) {
		this.jugador = jugador;
	}

	/**
	 * @uml.property  name="carta"
	 * @uml.associationEnd  multiplicity="(0 -1)" dimension="1" ordering="true" aggregation="composite" inverse="mundo:Carta"
	 * @uml.association  name="CartasMundo"
	 */
	private Carta[] cartas;

	/**
	 * Getter of the property <tt>carta</tt>
	 * @return  Returns the cartas.
	 * @uml.property  name="carta"
	 */
	public Carta[] getCarta() {
		return cartas;
	}

	/**
	 * Setter of the property <tt>carta</tt>
	 * @param carta  The cartas to set.
	 * @uml.property  name="carta"
	 */
	public void setCarta(Carta[] carta) {
		cartas = carta;
	}

}
